{
 "display_name": "Anaconda PySpark (Python 2 / Spark 1.3.1)",
 "language": "python",
 "argv": [
  "/opt/anaconda/bin/python",
  "-m",
  "IPython.kernel",
  "-f",
  "{connection_file}"
 ],
 "env": {
  "PYSPARK_PYTHON": "/opt/anaconda/bin/python",
  "SPARK_HOME": "/opt/spark-1.3.1-bin-hadoop2.6/",
  "PYTHONPATH": "python_modules/:/opt/python_modules/:/opt/spark-1.3.1-bin-hadoop2.6/python/:/opt/spark-1.3.1-bin-hadoop2.6/python/lib/py4j-0.8.2.1-src.zip",
  "PYTHONSTARTUP": "/opt/spark-1.3.1-bin-hadoop2.6/python/pyspark/shell.py",
  "PYSPARK_SUBMIT_ARGS": "--master 'local[*]' pyspark-shell"
 }
}
